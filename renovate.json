{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "local>jeichler/renovate-bot"
  ],
  "regexManagers": [
    {
      "fileMatch": [
        "^execution-environment\\.yml$"
      ],
      "matchStrings": [
        "EE_BASE_IMAGE: (?<depName>\\S*):(?<currentValue>\\S*)"
      ],
      "datasourceTemplate": "docker",
      "versioningTemplate": "semver"
    }
  ]
}
